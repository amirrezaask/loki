import "stdio.h";

// You can define structs like this
Human :: struct {
    id: int,
};

main :: fn() int {
    b1 :bool: true;
    b2 :bool: false;
    
    if b1 {
        printf("b1 is true");
    }

    if b2 {
        printf("b2 is true");    
    } else {
        printf("b2 is false");
    }

    return 0;
};
