#host "string"
#load "modules/print.loki"

Human :: struct {
      name: string,
      age: int
}

main :: () int {
     amirreza := Human { name = "amirreza", age = 12 };

     amirreza.age = 12;

     printf("amirreza age is %d\n", amirreza.age);
     printf("inline age is %d\n", Human {name = "comrade", age = 12 }.age);

}