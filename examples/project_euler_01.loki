#load "print"

is_multiple_of :: (x: s64, d: s64) bool {
	if (x % d == 0) {
		return true;
	}

	return false;
}

min :: (a: s64, b: s64) s64 {
	if (a > b) {
		return b;
	}

	return a;
}

main :: () int {
	threshold :: 1000;
	sum := 0;
	for (i := min(3, 5); i<threshold; i++) {
		if (is_multiple_of(i, 3) || is_multiple_of(i, 5)) {
			sum += i;
		}
	}

	printf("sum is %d\n", sum);

}